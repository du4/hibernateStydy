<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <appender review="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg%n</pattern>
    </encoder>
  </appender>




  <logger review="by.du4.study.hibernate" level="trace" />


  <root level="info">
    <appender-ref ref="STDOUT" />
  </root>

</configuration>